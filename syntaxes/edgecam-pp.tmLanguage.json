									"include": "#var"
								}
							]
						},
						"3": {
							"name": "keyword.operator.if.edgecam-pp"
						},
						"4": {
							"patterns": [
								{
									"include": "#sys-func"
								},
								{
									"include": "#var"
								},
								{
									"include": "#numbers"
								},
								{
									"include": "#strings"
								}
							]
						},
						"5": {
							"name": "keyword.other.label-call-if.edgecam-pp"
						},
						"6": {
							"name": "keyword.control.then.edgecam-pp"
						},
						"7": {
							"patterns": [
								{
									"include": "#single-line-proc"
								}
							]
						}
					}
				}
			]
		},
		"single-line-proc": {
			"patterns": [
