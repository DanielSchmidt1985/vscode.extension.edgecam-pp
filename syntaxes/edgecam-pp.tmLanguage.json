				{
					"match": "(?i)\\s*(%if)\\s+([#$]{0,1}[a-z0-9]+)\\s*(<>|<=|>=|=|<|>)\\s*([\"#$+-0-9]*\\.{0,1}[\"a-z0-9_-]*(?:[(]{1}[^)]*[)]{1}){0,1})\\s*(@[a-z0-9_-]*){0,1}(%then){0,1}\\s*(.*)",
					"captures": {
						"1": {
							"name": "keyword.control.if.edgecam-pp"
