				},
				"3": {
					"name": "entity.name.label.edgecam-pp"
				}
			},
			"name": "markup.italic.output.edgecam-pp"
		},
		"var": {
			"patterns": [
				{
					"match": "(?i)[#$][a-z0-9]*",
